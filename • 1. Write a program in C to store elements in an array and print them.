#include <stdio.h>

int main() {
    int n;
    int arr[100];
    printf("Numbers of elements you want to input ? ");
    scanf("%d",&n);
    printf("Input %d elements in the array :\n",n);
    for(int i=0;i<n;i++){
        printf("element - %d: ",i);
        scanf("%d",&arr[i]);
    }
     for(int i=0;i<n;i++){
        printf("%d ",arr[i]);

    }

    
   

    return 0;
}
